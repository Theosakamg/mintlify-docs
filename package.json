{
  "name": "upsun-docs",
  "version": "0.1.0",
  "type": "module",
  "description": "",
  "directories": {
    "doc": "dev/docs"
  },
  "scripts": {
    "install-dev": "npm i -g mint && ./scripts/install-dev.sh",
    "install:update": "mint update",
    "cache:generate": "node scripts/sync-readme.js",
    "cache:clear": "rm -rf contents/snippets/.cache/*",
    "cache:check:mdx": "node scripts/check-mdx.js contents/snippets/.cache",
    "cache:all": "npm run cache:clear && npm run cache:generate && npm run cache:check:mdx",
    "start": "npm run cache:generate && cd contents && mint dev --verbose --env-file ../.env",
    "build": "npm run cache:generate && echo TODO",
    "lint": "echo TODO",
    "lint:fix": "echo TODO",
    "format": "echo TODO",
    "format:fix": "echo TODO",
    "check:broken-links": "cd contents && mint broken-links",
    "check:a11y": "cd contents && mint a11y",
    "check:mdx": "node scripts/check-mdx.js contents",
    "check:openapi": "cd contents && mint openapi-check openapi.json",
    "check:all": "npm run check:mdx ; npm run check:openapi ; cd contents && npm run check:broken-links ; npm run check:a11y",
    "publish": "cd contents && mint publish --env-file ../.env",
    "migrate:vendor-name": "node scripts/replace-vendor-name.js",
    "migrate:vendor-urlraw": "node scripts/replace-vendor-urlraw.js",
    "all": "npm run install:update && npm run cache:all && npm run check:all && npm run start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Theosakamg/mintlify-docs.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Theosakamg/mintlify-docs/issues"
  },
  "homepage": "https://github.com/Theosakamg/mintlify-docs#readme",
  "devDependencies": {
    "@mdx-js/mdx": "^3.1.1",
    "dotenv": "^17.2.3",
    "mdx": "^0.3.1"
  }
}
