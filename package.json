{
  "name": "upsun-docs",
  "version": "0.1.0",
  "type": "module",
  "description": "",
  "directories": {
    "doc": "dev/docs"
  },
  "scripts": {
    "dev": "tsx src/index.ts",
    "build:ts": "tsc",
    "build:clean": "rm -rf dist",
    "build": "npm run build:clean && npm run build:ts",
    "type-check": "tsc --noEmit",
    "install-dev": "npm i -g mint && ./scripts/install-dev.sh",
    "install:update": "mint update",
    "install:autocomplete": "node ./bin/run.js autocomplete:script",
    "cache:generate": "node ./bin/run.js sync-readme",
    "cache:clear": "node ./bin/run.js clean",
    "cache:check:mdx": "node ./bin/run.js check:mdx contents/snippets/.cache",
    "cache:articles": "node ./bin/run.js blog:generate",
    "cache:all": "npm run cache:clear && npm run cache:generate && npm run cache:articles && npm run cache:check:mdx",
    "start": "npm run cache:generate && npm run cache:articles && cd contents && mint dev --verbose --env-file ../.env",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "check:broken-links": "cd contents && mint broken-links",
    "check:a11y": "cd contents && mint a11y",
    "check:header": "node ./bin/run.js check:header",
    "check:header:verbose": "node ./bin/run.js check:header --verbose",
    "check:mdx": "node ./bin/run.js check:mdx contents",
    "check:openapi": "cd contents && mint openapi-check openapi.json",
    "check:all": "npm run check:mdx ; npm run check:header ; npm run check:openapi ; cd contents && npm run check:broken-links ; npm run check:a11y",
    "publish": "cd contents && mint publish --env-file ../.env",
    "migrate:vendor-name": "node scripts/replace-vendor-name.js",
    "migrate:vendor-urlraw": "node scripts/replace-vendor-urlraw.js",
    "app:all": "npm run lint:fix && npm run format:fix && npm run build && npm run cache:all && npm run check:all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Theosakamg/mintlify-docs.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Theosakamg/mintlify-docs/issues"
  },
  "homepage": "https://github.com/Theosakamg/mintlify-docs#readme",
  "devDependencies": {
    "@eslint/compat": "^1.2.4",
    "@mdx-js/mdx": "^3.1.1",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^25.0.1",
    "dotenv": "^17.2.3",
    "eslint": "^9.17.0",
    "eslint-config-oclif": "^6.0.2",
    "eslint-config-prettier": "^9.1.0",
    "mdx": "^0.3.1",
    "prettier": "^3.4.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@oclif/core": "^4.8.0",
    "@oclif/plugin-autocomplete": "^3.2.39",
    "@oclif/prettier-config": "^0.2.1",
    "js-yaml": "^4.1.1",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.3"
  },
  "bin": {
    "upsun-docs": "./bin/run.js"
  },
  "oclif": {
    "bin": "upsun-docs",
    "commands": "./dist/commands",
    "hooks": "./dist/hooks",
    "dirname": "upsun-docs",
    "topicSeparator": ":",
    "plugins": [
      "@oclif/plugin-autocomplete"
    ]
  }
}
